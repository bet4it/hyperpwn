{
  "name": "hyperpwn",
  "version": "0.2.2",
  "description": "Build a pwn environment on hyper",
  "repository": "bet4it/hyperpwn",
  "author": "Bet4 <0xbet4@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "cfgs"
  ],
  "bin": {},
  "keywords": [
    "hyper"
  ],
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "max-depth": "off",
      "complexity": "off",
      "no-control-regex": "off",
      "padding-line-between-statements": [
        "error",
        {
          "blankLine": "any",
          "prev": "multiline-block-like",
          "next": "*"
        }
      ]
    },
    "env": [
      "node",
      "browser"
    ]
  },
  "devDependencies": {
    "xo": "^0.24.0"
  },
  "dependencies": {
    "ansi-escapes": "^4.2.0",
    "cli-truncate": "github:bet4it/cli-truncate",
    "expandtabs": "^1.0.0",
    "fs-extra": "^8.0.1",
    "lodash.merge": "^4.6.1",
    "strip-ansi": "^5.2.0"
  }
}
